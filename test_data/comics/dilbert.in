    function findAds(startPoint) {
        // Starting point
        var sp = document.querySelectorAll(startPoint);
        // Place to store ad slot divs
        var ads;

        // Find all ads after the starting point
        if(sp.length != 0) {
            ads = $(sp).find('.ad');
        } else {
            ads = document.querySelectorAll('.ad');
        }

        return ads;

    }


    function defineAds(adDivs) {
        var adDivsLength = adDivs.length;

        var adType, slotID, slot;

        // Place to store slot objects
        var adSlots = [];
        for (var i = adDivsLength-1; i>=0; i--) {
            // ID given to ad slot, usually ad type + index
            slotName = adDivs[i].id;

            adType = slotName.replace(/[0-9]/g, '');
            slotID = slotName.match(/\d+/g, '');

            slot = googletag.defineSlot('/19196947/dilbert.com/strips', adSizes[adType], slotName).defineSizeMapping(adSizeMappings[adType+'Mapping']).addService(googletag.pubads()).setTargeting('pos', adTargets[adType]);
            adSlots.push(slot);

            // If it is the anchored tower slot, store it
            // for later reference
            if(slotName == 'anchored_tower'){
                anchor = slot;
            }

        }
        return adSlots;
    }



    function displayAds(adDivs) {
        var adDivsLength = adDivs.length;
        var slotName;

        if(adDivsLength == 0) {
            googletag.display();
        } else {
            for (var i = adDivsLength-1; i>=0; i--) {
                slotName = adDivs[i].id;
                googletag.display(slotName);
            }
        }

    }



    function refreshAds(adSlots) {
        if (adSlots.length == 0) {
            googletag.pubads().refresh();
        } else {
            googletag.pubads().refresh(adSlots);
        }
    }

